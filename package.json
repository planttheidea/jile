{
  "author": "planttheidea",
  "ava": {
    "babel": "inherit",
    "failFast": true,
    "require": [
      "babel-register",
      "babel-polyfill",
      "./test/helpers/setup-browser-env.js"
    ],
    "files": [
      "./test/index.test.js"
    ]
  },
  "bugs": {
    "url": "https://github.com/planttheidea/jile/issues"
  },
  "dependencies": {
    "blob-polyfill": "^1.0.20150320",
    "hash-it": "^2.0.1",
    "inline-style-prefixer": ">=2.0.0",
    "lodash": "^4.16.6"
  },
  "description": "Styling library using the full power of both CSS and JS",
  "devDependencies": {
    "ava": "0.16.0",
    "babel": "6.5.2",
    "babel-cli": "6.18.0",
    "babel-eslint": "7.1.0",
    "babel-loader": "6.2.7",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-latest": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "eslint": "3.10.0",
    "eslint-friendly-formatter": "2.0.6",
    "eslint-loader": "1.6.1",
    "eslint-plugin-babel": "3.3.0",
    "eslint-plugin-react": "6.6.0",
    "html-webpack-plugin": "2.24.1",
    "in-publish": "2.0.0",
    "jsdom": "9.8.3",
    "jsdom-global": "^2.1.0",
    "lodash-webpack-plugin": "^0.10.5",
    "optimize-js-plugin": "0.0.4",
    "performance-now": "0.2.0",
    "react": ">=15.0.0",
    "react-dom": ">=15.0.0",
    "rimraf": "^2.5.4",
    "webpack": "1.13.3",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "1.16.2"
  },
  "homepage": "https://github.com/planttheidea/jile#readme",
  "keywords": [
    "style",
    "inline",
    "styles",
    "css",
    "radium",
    "css",
    "modules"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "name": "jile",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/jile.git"
  },
  "scripts": {
    "build": "NODE_ENV=development webpack --progress",
    "build:minified": "NODE_ENV=production webpack --progress --config=webpack.config.minified.js",
    "lint": "NODE_ENV=test eslint src",
    "prepublish": "in-publish && npm run prepublish:compile || in-install",
    "prepublish:compile": "npm run lint && npm run test && npm run transpile && npm run build && npm run build-minified",
    "start": "NODE_ENV=development webpack-dashboard -- webpack-dev-server --config=webpack.config.dev.js",
    "test": "NODE_ENV=test ava --no-cache",
    "test:timed": "NODE_ENV=test TIMED=true ava --no-cache",
    "test:watch": "NODE_ENV=test ava --no-cache --watch",
    "transpile": "NODE_ENV=production babel src --out-dir lib"
  },
  "version": "1.3.1"
}
