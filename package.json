{
  "author": "planttheidea",
  "ava": {
    "babel": "inherit",
    "failFast": true,
    "require": [
      "babel-register",
      "babel-polyfill",
      "./test/helpers/setup-browser-env.js"
    ],
    "files": [
      "./test/index.test.js"
    ]
  },
  "bugs": {
    "url": "https://github.com/planttheidea/jile/issues"
  },
  "dependencies": {
    "inline-style-prefixer": "^2.0.1",
    "lodash": "^4.13.1",
    "min.css": "^1.3.0"
  },
  "description": "Styling library using the full power of both CSS and JS",
  "devDependencies": {
    "ava": "^0.15.1",
    "babel": "^6.5.2",
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.0.1",
    "eslint-friendly-formatter": "^2.0.5",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-react": "^5.1.1",
    "html-webpack-plugin": "^2.17.0",
    "in-publish": "^2.0.0",
    "jsdom": "^9.2.0",
    "performance-now": "^0.2.0",
    "react": ">=15.0.0",
    "react-dom": ">=15.0.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "homepage": "https://github.com/planttheidea/jile#readme",
  "keywords": [
    "style",
    "inline",
    "styles",
    "css",
    "radium",
    "css",
    "modules"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "name": "jile",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/planttheidea/jile.git"
  },
  "scripts": {
    "build": "NODE_ENV=development ./node_modules/webpack/bin/webpack.js --progress",
    "build-minified": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --progress --config=webpack.config.minified.js",
    "compile-for-publish": "npm run lint && npm run test && npm run transpile && npm run build && npm run build-minified",
    "example": "NODE_ENV=development webpack-dev-server --progress --config=webpack.config.App.js",
    "example-minified": "NODE_ENV=production webpack-dev-server --progress --config=webpack.config.App.js",
    "lint": "NODE_ENV=test eslint src",
    "prepublish": "in-publish && npm run compile-for-publish || in-install",
    "test": "NODE_ENV=test ava --no-cache",
    "test:watch": "NODE_ENV=test ava --no-cache --watch",
    "test-timed": "NODE_ENV=test TIMED=true ava --no-cache",
    "transpile": "NODE_ENV=production ./node_modules/babel-cli/lib/babel/index.js -d lib/ src/"
  },
  "version": "1.3.0"
}
